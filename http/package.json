{
  "name": "@mockdog/http",
  "version": "0.0.0",
  "description": "MockDog HTTP Mock Server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "nodemon --config .nodemon.config.json",
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && tsc --build --force tsconfig.build.json",
    "test": "jest --detectOpenHandles",
    "test:fail": "jest --detectOpenHandles --onlyFailures",
    "lint": "eslint --fix --ext .ts ."
  },
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Vitor Hugo Salgado <vsalgadopb@gmail.com>",
  "homepage": "https://github.com/vitorsalgado/mockdog/tree/master/packages/http#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/mockdog.git"
  },
  "bugs": {
    "url": "https://github.com/vitorsalgado/mockdog/issues"
  },
  "dependencies": {
    "@mockdog/core": "^0.0.0",
    "@mockdog/matchers": "^0.0.0",
    "@mockdog/x": "^0.0.0",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.19",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "object-hash": "^3.0.0",
    "pino": "^8.6.0",
    "pino-pretty": "^9.1.0",
    "uuid": "^9.0.0",
    "yargs": "^17.5.1"
  },
  "peerDependencies": {
    "ts-node": "^10.2.1"
  }
}
